{
  "name": "@chaim/cli",
  "version": "0.1.0",
  "description": "Schema-driven code generation tool for DynamoDB",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "chaim": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "chaim",
    "cli",
    "code-generation",
    "dynamodb",
    "schema"
  ],
  "author": "Chaim Builder",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-sts": "^3.883.0",
    "@chaim/cdk": "file:../../chaim-cdk",
    "@chaim/chaim-bprint-spec": "file:../../chaim-bprint-spec",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.0.0",
    "ts-node": "^10.9.0",
    "typescript": "~5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chaim-builder/chaim-cli.git"
  },
  "bugs": {
    "url": "https://github.com/chaim-builder/chaim-cli/issues"
  },
  "homepage": "https://github.com/chaim-builder/chaim-cli#readme"
}
